{
  "name": "visualdebugger",
  "displayName": "VisualDebugger",
  "description": "A Visual Debugger for C. Shows memory, pointers, and stack and heap data.",
  "version": "0.0.1",
  "publisher": "lilliannedavis",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "execute_gdb",
        "title": "VDBUG: Execute GDB"
      },
      {
        "command": "visualdebugger.buildWithMemlog",
        "title": "VDBUG: Build with Memlog Plugin"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "IntroView",
          "title": "Visual Debugger",
          "icon": "$(debug-console)" 
        }
      ]
    },
    "views": {
      "IntroView": [
        {
          "id": "first_panel",
          "name": "Dashboard"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "first_panel",
        "contents": "[Start Debugger](command:execute_gdb)\n[Build with Plugin](command:visualdebugger.buildWithMemlog)"
      }
    ]
  }
}